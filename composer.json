{
  "name": "s-mcdonald/jason",
  "description": "Jason (JSON) is a PHP library that allows you to serialize classes with attributes, build Json structures with a builder and much more.",
  "keywords": ["json-builder", "JsonBuilder", "Json Decoder", "Json Utilities", "json", "serializer", "JsonSerializer"],
  "homepage": "https://github.com/s-mcdonald/Jason",
  "license": "MIT",
  "version": "1.2.0",
  "authors": [
    {
      "name": "Sam McDonald",
      "email": "s.mcdonald@outlook.com.au",
      "homepage": "https://github.com/s-mcdonald"
    }
  ],
  "require": {
    "php": ">=8.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "mockery/mockery": "1.5.1"
  },
  "autoload": {
    "psr-4": {
      "SamMcDonald\\Jason\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\SamMcDonald\\Jason\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "scripts": {
    "build": [
      "composer dumpautoload",
      "composer update",
      "composer install"
    ],
    "tests": [
      "phpunit -c ./phpunit.xml ./tests"
    ]
  }
}
